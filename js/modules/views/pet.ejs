<div class="pet__container"><div class="pet__wrap"><div class="pet-name"><h2 class="pet-name__text"><%= model.get('petName') %></h2></div><div class="pet-photo"><div class="pet-photo__placeholder" style="background-image: url('<%= (model.get('images').length > 0) ? model.get('images')[0] : 'http://placehold.it/480x480' %>')"></div></div><div class="pet-info"><div class="pet-info__wrap"><div class="pet-info-breed"><p class="pet-info-breed__text"><%= model.get('breed') %></p></div><div class="pet-info-size"><p class="pet-info-size__text"><%= model.get('size') %></p></div><div class="pet-info-sex"><p class="pet-info-sex__text"><%= model.get('sex') %></p></div></div></div><div class="pet-details" data-details-id="<%= model.get('petId') %>"><div class="pet-details__wrap"><button class="remodal-close" data-remodal-action="close"></button><table class="petProp"><thead><td>Name</td><td>Details</td></thead><% for(var propName in model.attributes) {
    if(model.attributes.hasOwnProperty(propName)){ %><tr><td><%= propName %></td><td><%= model.get(propName) %></td></tr><%     };
}; %></table><button class="remodal-confirm" data-remodal-action="confirm">OK</button></div></div><div class="btn--more-info"><a class="btn__link" href="#<%= model.get('petId') %>">More Info</a></div></div></div>